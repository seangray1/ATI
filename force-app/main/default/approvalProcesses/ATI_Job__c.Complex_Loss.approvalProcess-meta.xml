            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>ATI_Job__c.Amount__c</field>
                <operation>lessThan</operation>
                <value>250000</value>
            </criteriaItems>
            <criteriaItems>
                <field>ATI_Job__c.Total_Billing_Forecast_Amount__c</field>
