            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>ATI_Job__c.Estimated_Gross_Profit_RS__c</field>
                <operation>lessThan</operation>
                <value>30</value>
            </criteriaItems>
        </entryCriteria>
        <label>Estimated GP Less than 30%</label>
        <name>Estimated_GP_Less_than_30</name>
