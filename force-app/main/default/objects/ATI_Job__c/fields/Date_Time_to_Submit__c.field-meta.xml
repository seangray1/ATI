<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Time_to_Submit__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
ISBLANK(Date_Time_Submitted_for_Approval__c),
(TIMEVALUE(NOW()) - TIMEVALUE(Date_Time_Taken__c))/60000,
(TIMEVALUE(Date_Time_Submitted_for_Approval__c) -  TIMEVALUE(Date_Time_Taken__c))/60000
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Date/Time to Submit (Minutes)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
